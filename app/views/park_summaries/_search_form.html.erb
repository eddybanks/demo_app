<div class="search-form" id="park-summary-search-form">
  <div class="form-inline input-group">
    <%= f.search_field :park_name_cont, class: 'form-control', placeholder: 'Park Name' %>
    <%= f.search_field :park_type_cont, class: 'form-control', placeholder: 'Choose Column' %>
    <%= f.search_field :email_cont, class: 'form-control', placeholder: 'Order By' %>
    <div class="input-group-btn">
      <%= f.submit 'Submit', class: 'btn btn-default' %>
    </div>
  </div>
</div>
 
